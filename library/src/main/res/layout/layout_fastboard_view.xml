<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:parentTag="android.widget.FrameLayout">

    <com.herewhite.sdk.WhiteboardView
        android:id="@id/fast_whiteboard_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center" />

    <io.agora.board.fast.ui.OverlayLayout
        android:id="@+id/fast_overlay_handle_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

    <FrameLayout
        android:id="@id/fast_room_controller"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <io.agora.board.fast.ui.LoadingLayout
        android:id="@id/fast_loading_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/fast_day_night_bg"
        android:gravity="center"
        android:orientation="vertical" />

    <io.agora.board.fast.ui.ErrorHandleLayout
        android:id="@id/fast_error_handle_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/fast_day_night_bg"
        android:gravity="center"
        android:orientation="vertical" />
</merge>